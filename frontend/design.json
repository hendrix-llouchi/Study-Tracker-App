        "header": {
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "center",
          "marginBottom": "20px"
        },
        "title": {
          "fontSize": "16px",
          "fontWeight": "600",
          "color": "#111827"
        },
        "sectionTitle": {
          "fontSize": "14px",
          "fontWeight": "600",
          "color": "#111827",
          "marginTop": "24px",
          "marginBottom": "12px"
