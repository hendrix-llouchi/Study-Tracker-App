          "color": "#6B7280",
          "marginBottom": "8px"
        },
        "classTime": {
          "fontSize": "12px",
          "color": "#F59E0B",
          "fontWeight": "500",
          "display": "flex",
          "alignItems": "center",
          "gap": "4px"
        },
        "liveButton": {
          "background": "#10B981",
          "color": "#FFFFFF",
          "padding": "6px 16px",
          "borderRadius": "6px",
          "fontSize": "12px",
          "fontWeight": "500",
          "border": "none",
          "cursor": "pointer",
          "marginLeft": "auto",
          "alignSelf": "center",
          "transition": "background 0.2s ease",
          "hoverBackground": "#059669"
        }
      },

