        "dayIconColors": {
          "active": "#F59E0B",
          "completed": "#10B981",
          "inactive": "#D1D5DB"
        },
        "dayLabel": {
          "fontSize": "11px",
          "fontWeight": "500"
        },
        "dayLabelColors": {
