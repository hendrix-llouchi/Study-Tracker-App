
{
  "designSystem": {
    "metadata": {
      "productName": "Edupro",
      "designLanguage": "Modern Educational Dashboard",
      "colorScheme": "Light mode with accent gradients",
      "primaryFont": "Inter, SF Pro Display, or similar sans-serif"
    },

    "colorPalette": {
      "primary": {
        "green": "#34D399",
        "greenHover": "#10B981",
        "greenLight": "#D1FAE5",
        "greenBackground": "#ECFDF5"
      },
      "secondary": {
        "blue": "#3B82F6",
        "blueLight": "#DBEAFE"
      },
      "accent": {
        "orange": "#F59E0B",
        "orangeLight": "#FEF3C7",
        "purple": "#A855F7",
        "purpleLight": "#F3E8FF",
        "pink": "#EC4899",
        "red": "#EF4444"
      },
      "neutral": {
        "white": "#FFFFFF",
        "gray50": "#F9FAFB",
        "gray100": "#F3F4F6",
        "gray200": "#E5E7EB",
        "gray300": "#D1D5DB",
        "gray400": "#9CA3AF",
        "gray500": "#6B7280",
        "gray600": "#4B5563",
        "gray700": "#374151",
        "gray800": "#1F2937",
        "gray900": "#111827"
      },
      "text": {
        "primary": "#111827",
        "secondary": "#6B7280",
        "tertiary": "#9CA3AF",
        "onPrimary": "#FFFFFF",
        "success": "#10B981",
        "warning": "#F59E0B",
        "error": "#EF4444"
      },
      "borders": {
        "default": "#E5E7EB",
        "light": "#F3F4F6",
        "medium": "#D1D5DB",
        "focus": "#34D399"
      }
