    "typography": {
      "fontFamily": {
        "primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "mono": "'JetBrains Mono', 'Fira Code', monospace"
      },
      "scale": {
        "h1": {
          "fontSize": "24px",
          "fontWeight": "700",
          "lineHeight": "32px",
          "color": "#111827"
        },
        "h2": {
          "fontSize": "20px",
          "fontWeight": "600",
          "lineHeight": "28px",
          "color": "#111827"
        },
        "h3": {
          "fontSize": "16px",
          "fontWeight": "600",
          "lineHeight": "24px",
          "color": "#111827"
        },
        "body": {
          "fontSize": "14px",
          "fontWeight": "400",
          "lineHeight": "20px",
          "color": "#374151"
        },
        "bodySmall": {
          "fontSize": "12px",
          "fontWeight": "400",
          "lineHeight": "16px",
          "color": "#6B7280"
        },
        "caption": {
          "fontSize": "11px",
          "fontWeight": "400",
          "lineHeight": "14px",
          "color": "#9CA3AF"
        },
        "button": {
          "fontSize": "14px",
          "fontWeight": "500",
          "lineHeight": "20px",
          "letterSpacing": "0.01em"
        }
      }
    },

    "spacing": {
      "unit": "4px",
      "scale": {
        "xs": "4px",
        "sm": "8px",
        "md": "12px",
        "lg": "16px",
        "xl": "24px",
        "2xl": "32px",
        "3xl": "48px"
      },
      "containerPadding": {
        "mobile": "16px",
        "tablet": "24px",
        "desktop": "32px"
      }
    },

    "borderRadius": {
      "none": "0px",
      "sm": "4px",
      "md": "8px",
      "lg": "12px",
      "xl": "16px",
      "2xl": "24px",
      "full": "9999px"
    },

    "shadows": {
      "none": "none",
      "xs": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)"
    },

    "components": {
      "layout": {
        "mainContainer": {
          "background": "#F9FAFB",
          "minHeight": "100vh",
          "padding": "24px"
        },
        "contentArea": {
          "maxWidth": "1440px",
          "margin": "0 auto",
          "display": "grid",
          "gap": "24px"
        },
        "sidebar": {
          "background": "#FFFFFF",
          "width": "240px",
          "padding": "24px 16px",
          "borderRight": "1px solid #E5E7EB",
          "position": "fixed",
          "height": "100vh"
        }
      },

      "navigationSidebar": {
        "container": {
          "background": "#FFFFFF",
          "borderRight": "1px solid #E5E7EB",
          "padding": "24px 0"
        },
        "logo": {
          "container": {
            "padding": "0 16px 24px 16px",
            "marginBottom": "24px"
          },
          "icon": {
            "background": "#34D399",
            "width": "32px",
            "height": "32px",
            "borderRadius": "8px",
            "display": "flex",
            "alignItems": "center",
            "justifyContent": "center"
          },
          "iconColor": "#FFFFFF",
          "text": {
            "fontSize": "18px",
            "fontWeight": "600",
            "color": "#111827",
            "marginLeft": "12px"
          }
        },
        "menuItem": {
          "default": {
            "padding": "10px 16px",
            "marginBottom": "4px",
            "borderRadius": "8px",
            "cursor": "pointer",
            "transition": "all 0.2s ease",
            "display": "flex",
            "alignItems": "center",
            "gap": "12px"
          },
          "defaultState": {
            "background": "transparent",
            "color": "#6B7280",
            "icon": "#9CA3AF"
          },
          "hoverState": {
            "background": "#F3F4F6",
            "color": "#111827",
            "icon": "#374151"
          },
          "activeState": {
            "background": "#ECFDF5",
            "color": "#059669",
            "icon": "#34D399",
            "borderLeft": "3px solid #34D399",
            "paddingLeft": "13px"
          },
          "icon": {
            "width": "20px",
            "height": "20px",
            "strokeWidth": "2px"
          },
          "text": {
            "fontSize": "14px",
            "fontWeight": "500"
          }
        },
        "section": {
          "label": {
            "fontSize": "11px",
            "fontWeight": "600",
            "color": "#9CA3AF",
            "textTransform": "uppercase",
            "letterSpacing": "0.05em",
            "padding": "16px 16px 8px 16px"
          }
        }
      },

      "topHeader": {
        "container": {
          "background": "#FFFFFF",
          "borderBottom": "1px solid #E5E7EB",
          "padding": "16px 32px",
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "center"
        },
        "greeting": {
          "fontSize": "14px",
          "color": "#6B7280"
        },
        "userName": {
          "fontSize": "16px",
          "fontWeight": "600",
          "color": "#111827"
        },
        "avatar": {
          "width": "40px",
          "height": "40px",
          "borderRadius": "9999px",
          "border": "2px solid #E5E7EB",
          "overflow": "hidden"
        },
        "icons": {
          "width": "20px",
          "height": "20px",
          "color": "#6B7280",
          "cursor": "pointer",
          "transition": "color 0.2s ease",
          "hoverColor": "#111827"
        }
      },

      "alertBanner": {
        "container": {
          "background": "#ECFDF5",
          "border": "1px solid #A7F3D0",
          "borderRadius": "12px",
          "padding": "16px 20px",
          "display": "flex",
          "alignItems": "center",
          "gap": "12px",
          "marginBottom": "24px"
        },
        "icon": {
          "width": "20px",
          "height": "20px",
          "color": "#059669",
          "flexShrink": "0"
        },
        "text": {
          "fontSize": "14px",
          "color": "#065F46",
          "lineHeight": "20px"
        },
        "closeButton": {
          "width": "20px",
          "height": "20px",
          "color": "#059669",
          "cursor": "pointer",
          "marginLeft": "auto",
          "hoverColor": "#047857"
        }
      },

      "statsCards": {
        "container": {
          "display": "grid",
          "gridTemplateColumns": "repeat(auto-fit, minmax(200px, 1fr))",
          "gap": "16px",
          "marginBottom": "24px"
        },
        "card": {
          "background": "#FFFFFF",
          "border": "1px solid #E5E7EB",
          "borderRadius": "16px",
          "padding": "20px",
          "transition": "all 0.3s ease",
          "cursor": "pointer"
        },
        "cardHover": {
          "transform": "translateY(-2px)",
          "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
          "borderColor": "#D1D5DB"
        },
        "iconContainer": {
          "width": "48px",
          "height": "48px",
          "borderRadius": "12px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "marginBottom": "12px"
        },
        "iconContainerColors": {
          "blue": "#DBEAFE",
          "green": "#D1FAE5",
          "orange": "#FEF3C7",
          "purple": "#F3E8FF"
        },
        "icon": {
          "width": "24px",
          "height": "24px"
        },
        "iconColors": {
          "blue": "#3B82F6",
          "green": "#10B981",
          "orange": "#F59E0B",
          "purple": "#A855F7"
        },
        "label": {
          "fontSize": "13px",
          "color": "#6B7280",
          "marginBottom": "4px"
        },
        "value": {
          "fontSize": "24px",
          "fontWeight": "700",
          "color": "#111827",
          "marginBottom": "8px"
        },
        "progressRing": {
          "size": "40px",
          "strokeWidth": "4px",
          "trackColor": "#E5E7EB",
          "progressColors": {
            "blue": "#3B82F6",
            "green": "#10B981",
            "orange": "#F59E0B",
            "purple": "#A855F7"
          }
        }
      },

      "performanceGauge": {
        "container": {
          "background": "#FFFFFF",
          "border": "1px solid #E5E7EB",
          "borderRadius": "16px",
          "padding": "24px"
        },
        "title": {
          "fontSize": "16px",
          "fontWeight": "600",
          "color": "#111827",
          "marginBottom": "20px"
        },
        "gaugeContainer": {
          "position": "relative",
          "width": "200px",
          "height": "100px",
          "margin": "0 auto"
        },
        "gaugeTrack": {
          "stroke": "#E5E7EB",
          "strokeWidth": "12px",
          "fill": "none"
        },
        "gaugeProgress": {
          "stroke": "#10B981",
          "strokeWidth": "12px",
          "fill": "none",
          "strokeLinecap": "round",
          "transition": "stroke-dashoffset 1s ease"
        },
        "percentageText": {
          "fontSize": "36px",
          "fontWeight": "700",
          "color": "#111827",
          "textAlign": "center"
        },
        "label": {
          "fontSize": "14px",
          "color": "#6B7280",
          "textAlign": "center",
          "marginTop": "8px"
        }
      },

      "courseList": {
        "container": {
          "background": "#FFFFFF",
          "border": "1px solid #E5E7EB",
          "borderRadius": "16px",
          "padding": "24px"
