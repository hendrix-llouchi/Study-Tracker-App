          "alignItems": "center",
          "justifyContent": "space-between",
          "transition": "background 0.2s ease"
        },
        "courseRowHover": {
          "background": "#F9FAFB",
          "borderRadius": "8px",
          "padding": "16px 12px"
        },
        "courseIcon": {
          "width": "40px",
          "height": "40px",
          "borderRadius": "8px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "marginRight": "12px"
        },
        "courseIconColors": {
          "orange": "#FEF3C7",
          "blue": "#DBEAFE",
          "green": "#D1FAE5",
          "purple": "#F3E8FF",
          "pink": "#FCE7F3"
        },
        "courseName": {
          "fontSize": "14px",
          "fontWeight": "500",
          "color": "#111827"
        },
        "courseDetails": {
